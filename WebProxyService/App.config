<?xml version="1.0" encoding="utf-8" ?>

<configuration>
	<system.serviceModel>
		<services>
			<service name="WebProxyService.ProxyService">
				<endpoint address="http://localhost:8733/Design_Time_Addresses/WebProxyService/ProxyService/" binding="webHttpBinding" contract="WebProxyService.IProxyService"/>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8733/Design_Time_Addresses/WebProxyService/ProxyService/" />
					</baseAddresses>
				</host>
			</service>

		</services>
		<behaviors>
			<endpointBehaviors>
